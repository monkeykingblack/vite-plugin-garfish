{
  "name": "vite-plugin-garfish",
  "version": "1.0.0",
  "description": "A vite plugin for running Micro frontend applications using garfish",
  "keywords": [
    "@garfish/react-bridge",
    "garfish",
    "micro frontend",
    "micro frontends",
    "micro-frontend",
    "micro-frontends",
    "microfrontend",
    "microfrontends",
    "react",
    "vite",
    "vite-plugin",
    "vite-plugin-garfish"
  ],
  "license": "MIT",
  "author": "monkeykingblack",
  "files": [
    "./dist",
    "./src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "development": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsdown"
  },
  "dependencies": {
    "cheerio": "^1.2.0"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.9",
    "domhandler": "^5.0.3",
    "tsdown": "^0.20.3"
  },
  "peerDependencies": {
    "vite": ">= 4 < 8"
  }
}
